<?xml version="1.0" encoding="ISO-8859-1" ?>
<table name="newsletter_mailings" prefix="yes">
  <fields>
    <field name="mailing_id" type="integer" size="8" null="no" autoinc="yes"/>
    <field name="mailinggroup_id" type="integer" size="4" null="no"/>
    <field name="lng_id" type="integer" size="4" null="no"/>
    <field name="author_id" type="string" size="32" null="no"/>
    <field name="mailing_title" type="string" size="250" null="no"/>
    <field name="mailing_intro" type="string" size="65535" null="no"/>
    <field name="mailing_footer" type="string" size="65535" null="no"/>
    <field name="mailing_intro_nl2br" type="integer" size="4" null="no"/>
    <field name="mailing_footer_nl2br" type="integer" size="4" null="no"/>
    <field name="mailing_note" type="string" size="250" null="no"/>
    <field name="mailing_url" type="string" size="250" null="no"/>
    <field name="unsubscribe_url" type="string" size="250" null="no"/>
    <field name="mailing_created" type="integer" size="8" null="no"/>
    <field name="mailing_modified" type="integer" size="8" null="no"/>
    <field name="mailing_protected" type="integer" size="2" null="no"/>
  </fields>
  <keys>
    <primary-key>
      <field>mailing_id</field>
    </primary-key>
  </keys>
</table>
